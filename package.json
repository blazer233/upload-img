{
  "name": "kf-upload-img",
  "displayName": "upload img",
  "icon": "assets/icon.png",
  "description": "right click here, short cut here, upload img",
  "version": "2.1.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "publisher": "daryslong",
  "categories": [
    "Other"
  ],
  "keywords": [
    "upload",
    "image",
    "insert",
    "flash"
  ],
  "activationEvents": [
    "onCommand:kf-upload-img.UploadHere"
  ],
  "repository": "https://github.com/blazer233/upload-img",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kf-upload-img.UploadHere",
        "title": "upload img"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "kf-upload-img.UploadHere",
          "alt": "kf-upload-img.UploadHere",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "upload img configuration",
      "properties": {
        "upload img.uploadUrl": {
          "type": "string",
          "default": "D:\\work_code\\weapp-image\\dist\\weapp-workbench\\images",
          "description": "指定上传图片git本地地址"
        },
        "upload img.targetSrc": {
          "type": "string",
          "default": "images",
          "description": "指定上传图片文件夹目录"
        }
      }
    },
    "keybindings": [
      {
        "command": "kf-upload-img.UploadHere",
        "key": "shift+alt+p",
        "mac": "shift+alt+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "simple-git": "^3.7.1"
  }
}